<?xml version="1.0" encoding="UTF-8"?>

<numericalExperiment documentVersion="3" control="false"
    xmlns="http://www.metaforclimate.eu/cim/1.1"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.metaforclimate.eu/cim/1.1 C:\Users\clp73\Desktop\Metafor\subversion\CIM\branches\dev0p3\activity.xsd"
    xmlns:xlink="http://www.w3.org/1999/xlink" 
    xmlns:gmd="http://www.isotc211.org/2005/gmd"
    xmlns:gco="http://www.isotc211.org/2005/gco">

    <rationale> Historical simulation (1850 to 2005) with imposed changing conditions (consistent
        with observations). </rationale>
    <Why> Evaluate model performance against present climate and observed climate change </Why>
    <!-- Historical (1850-2005).
        Impose changing conditions (consistent with observations), which may include:
        * atmospheric composition - including [CO2]
        - due to anthropogenic influence
        - due to volcanic influence
        * solar forcing
        * emissions or concentrations of
        - short lived species
        - aerosols
        >natural aerosols or their precursors
        >anthropogenic aerosols or their precursors
        * Land Use -->

    <!-- Initial Conditions -->
    <numericalRequirement xsi:type="InitialCondition">
        <description> Initial conditions are from experiment 3.1.PreIndustrialControl </description>
        <id xsi:type="Identifier">3.2.ic</id>
        <!-- 3.2.ic -->
        <!-- 6.1.ic -->
        <!-- 6.2a.ic -->
        <!-- 6.2b.ic -->
    </numericalRequirement>

    <!-- Boundary/Forcing Conditions -->
    <numericalRequirement xsi:type="BoundaryCondition">
        <description>Imposed changing concentrations of anthropogenic well mixed gases, including CO2</description>
        <id xsi:type="Identifier">3.2.bc.wmg_anth.conc</id>
    </numericalRequirement>
    <numericalRequirement xsi:type="BoundaryCondition">
        <description>Imposed changing concentrations of volcanic well mixed gases, including CO2</description>
        <id xsi:type="Identifier">3.2.bc.wmg_volc.conc</id>
    </numericalRequirement>
    <numericalRequirement xsi:type="BoundaryCondition">
        <description>Imposed changing concentrations of short lived gas species</description>
        <id xsi:type="Identifier">3.2.bc.sls.conc</id>
    </numericalRequirement>
    <numericalRequirement xsi:type="BoundaryCondition">
        <description>Imposed changing concentrations of natural aerosols</description>
        <id xsi:type="Identifier">3.2.bc.aer_nat.conc</id>
    </numericalRequirement>
    <numericalRequirement xsi:type="BoundaryCondition">
        <description>Imposed changing concentrations of anthropogenic aerosols</description>
        <id xsi:type="Identifier">3.2.bc.aer_anth.conc</id>
    </numericalRequirement>
    <numericalRequirement xsi:type="BoundaryCondition">
        <description>Imposed changing concentrations of natural aerosol precursors</description>
        <id xsi:type="Identifier">3.2.bc.aer_pre_nat.conc</id>
    </numericalRequirement>
    <numericalRequirement xsi:type="BoundaryCondition">
        <description>Imposed changing concentrations of anthropogenic aerosol
            precursors</description>
        <id xsi:type="Identifier">3.2.bc.aer_pre_anth.conc</id>
    </numericalRequirement>
    <numericalRequirement xsi:type="BoundaryCondition">
        <description>Imposed changing emissions of short lived gas species</description>
        <id xsi:type="Identifier">3.2.bc.sls.em</id>
    </numericalRequirement>
    <numericalRequirement xsi:type="BoundaryCondition">
        <description>Imposed changing emissions of natural aerosols </description>
        <id xsi:type="Identifier">3.2.bc.aer_nat.em</id>
    </numericalRequirement>
    <numericalRequirement xsi:type="BoundaryCondition">
        <description>Imposed changing emissions of anthropogenic aerosols</description>
        <id xsi:type="Identifier">3.2.bc.aer_anth.em</id>
    </numericalRequirement>
    <numericalRequirement xsi:type="BoundaryCondition">
        <description>Imposed changing solar forcing</description>
        <id xsi:type="Identifier">3.2.bc.solar</id>
    </numericalRequirement>
    <numericalRequirement xsi:type="BoundaryCondition">
        <description>Imposed changing land use</description>
        <id xsi:type="Identifier">3.2.bc.LandUse</id>
    </numericalRequirement>

    <CalendarType>
        <realCalendar>
            <range>1850-2005</range>
        </realCalendar>
    </CalendarType>
    <requiredDuration xsi:type="ClosedDateRange">
        <startDate>1850-01-01T00:00:00Z</startDate>
        <endDate>2006-01-01T00:00:00Z</endDate>
        <lengthYears>156</lengthYears>
    </requiredDuration>
    <author>Charlotte Pascoe</author>
    <creationDate>2009-06-26T14:48:00Z</creationDate>
    <documentID xsi:type="Identifier">3.2_historical </documentID>

</numericalExperiment>
