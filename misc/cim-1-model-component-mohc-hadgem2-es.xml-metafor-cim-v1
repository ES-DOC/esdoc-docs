                                <value>centered finite differences</value>
                            </componentProperty>
                            <!--Constraint end: if SchemeType is "fixed grid"-->
                            <!--Constraint start: if SchemeMethod is "finite differences" or "centered finite differences"-->
                            <componentProperty represented="true">
                                <shortName>SchemeOrder</shortName>
                                <longName>SchemeOrder</longName>
                                <value>second order</value>
                            </componentProperty>
                            <!--Constraint end: if SchemeMethod is "finite differences" or "centered finite differences"-->
                        </componentProperty>
                        <componentProperty represented="true">
                            <shortName>HorizontalDiffusion</shortName>
                            <longName>HorizontalDiffusion</longName>
                            <componentProperty represented="true">
