                        <longName>AerosolSchemeScope</longName>
                        <value>whole atmosphere</value>
                    </componentProperty>
                    <componentProperty represented="true">
                        <shortName>BasicApproximations</shortName>
                        <longName>BasicApproximations</longName>
                        <value>Modal scheme, mass as a tracer, number inferred from prescribed size distributions</value>
                    </componentProperty>
                    <componentProperty represented="true">
                        <shortName>ListOfPrognosticVariables</shortName>
                        <longName>ListOfPrognosticVariables</longName>
                        <value>3D mass/volume mixing ratio for aerosols</value>
                    </componentProperty>
